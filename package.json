{
  "name": "webpack2test",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf ./public/built",
    "build:dev": "npm run clean && webpack --config config/webpack.config.js",
    "start:dev": "npm run build:dev && NODE_ENV=development ./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node ./src/server/index.js --presets es2015,stage-0,react",
    "build:prod": "npm run clean && webpack -p --config config/webpack.config.js",
    "start:prod": "export NODE_ENV=production && npm run build:prod && NODE_ENV=production node ./src/server/index.js",
    "jest": "NODE_ENV=test BABEL_DISABLE_CACHE=1 ./node_modules/.bin/jest --no-cache",
    "jest:watch": "npm run jest -- --watch",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src",
    "test": "npm run jest && npm run lint",
    "test:watch": "npm run jest:watch"
  },
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "eslint-import-resolver-webpack": "^0.8.0",
    "express": "5.0.0-alpha.3",
    "hpp": "^0.2.1",
    "isomorphic-webpack": "^2.0.2",
    "moment": "^2.17.1",
    "nodemon": "^1.11.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "spdy": "^3.4.4",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-react-css-modules": "^2.6.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.10.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "postcss-color-function": "^3.0.0",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.0",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.1",
    "style-loader": "0.13.1",
    "uglify-js": "^2.8.12",
    "url-loader": "^0.5.8"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/tests/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "/node_modules",
      "/src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "pre-commit": [
    "jest",
    "lint"
  ]
}
